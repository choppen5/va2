Public Sub CreateEventWizard(ByVal PropertySheet As SnapInLib.MMCPropertySheet, Optional ByVal AnalysisRuleID As Long, Optional ByVal AnalysisRuleName As String)
    Set EventWzData = New clsEventWzData
    If AnalysisRuleID > 0 Then
        With EventWzData
        .AnRuleID = AnalysisRuleID
        .AnRuleName = AnalysisRuleName
        .FromAnRule = True
        End With
    End If
    With PropertySheet
        .AddWizardPage "ppgWzErAnRule", EventWzData
        .AddWizardPage "ppgWzErErrDef", EventWzData
        .AddWizardPage "ppgWzErNRule", EventWzData
        .AddWizardPage "ppgWzErReaction", EventWzData
        .AddWizardPage "ppgWzErFinish", EventWzData
    End With
End Sub

Private Sub RefreshNode(ByVal NodeTypeName As String, Optional ByVal ScopeItem As SnapInLib.ScopeItem)

'    Dim ScopeItem As ScopeItem

    Select Case NodeTypeName
    Case "nd_notification_rule_root"    'Notification Rules
        Set ScopeItem = ScopeItems.Item(NodeTypeName)
        If ScopeItem.ScopeNode.ExpandedOnce Then
            ScopeItem.RemoveChildren
            Call fill_notification_rules(ScopeItem)
        End If
    Case "nd_reaction_root"             'Reaction
        Set ScopeItem = ScopeItems.Item(NodeTypeName)
        If ScopeItem.ScopeNode.ExpandedOnce Then
            ScopeItem.RemoveChildren
            Call fill_reaction(ScopeItem)
        End If
    Case "nd_sft_error_defs_root"       'Error Definition
        Set ScopeItem = ScopeItems.Item(NodeTypeName)
        If ScopeItem.ScopeNode.ExpandedOnce Then
            ScopeItem.RemoveChildren
            Call fill_sft_error_defs(ScopeItem)
        End If
    Case "nd_comunicationserver_root"   'Communication Server
        Set ScopeItem = ScopeItems.Item(NodeTypeName)
        If ScopeItem.ScopeNode.ExpandedOnce Then
            ScopeItem.RemoveChildren
            Call fill_comunicationserver(ScopeItem)
        End If
    Case "nd_administrators_root"       'Administrators
        Set ScopeItem = ScopeItems.Item(NodeTypeName)
        If ScopeItem.ScopeNode.ExpandedOnce Then
            ScopeItem.RemoveChildren
            Call fill_administrators(ScopeItem)
        End If
    Case "nd_schedule_root"             'Schedules
        Set ScopeItem = ScopeItems.Item(NodeTypeName)
        If ScopeItem.ScopeNode.ExpandedOnce Then
            ScopeItem.RemoveChildren
            Call fill_schedule(ScopeItem)
        End If
    End Select
End Sub


