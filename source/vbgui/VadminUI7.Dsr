Private Sub Views_Delete(ByVal View As SnapInLib.View, ByVal Selection As SnapInLib.MMCClipboard)
    Dim dobj As New vaDataAccess
    Dim pid As Integer
    Dim IsOK As Boolean

'Undeclared
Dim Choice
    Choice = ConsoleMsgBox("Are you sure you want to delete the selected item?", _
                           vbYesNo, "Delete")
    If Choice = vbNo Then
        Exit Sub
    End If

    IsOK = True

    Select Case Selection.SelectionType
    Case siSingleScopeItem
        pid = splitval(Selection.ScopeItems(1).Key, "~", 1)
        Debug.Print "views delete scopeitem key = " & Selection.ScopeItems(1).Key

        Select Case Selection.ScopeItems(1).ScopeNode.NodeTypeName
            
            Case "nd_components"
                Call dobj.returndataset("delete from comp_errdef where components_id = " & pid)
                Call dobj.returndataset("delete from components where components_id = " & pid)
                
            Case "nd_sft_mng_sys"
                 Call dobj.del_child_sft_mng_sys(pid)
        
            Case "nd_sft_elmnt"
                 Call dobj.del_child_sft_elmnts(pid)
        
            Case "nd_host"
                 Call dobj.returndataset("delete from host where host_id = " & pid)
        
            Case "nd_sft_error_defs"
                Call dobj.returndataset("delete from analysis_errdef where error_defs_id = " & pid)
                Call dobj.returndataset("delete from comp_errdef where error_defs_id = " & pid)
                Call dobj.returndataset("delete from sft_err_deff where error_defs_id = " & pid)
                Call dobj.returndataset("delete from sft_error_defs where error_defs_id = " & pid)
        
            Case "nd_notification_rule"
                Call dobj.returndataset("delete from notification_reaction where note_rule_id = " & pid)
                Call dobj.returndataset("delete from notification_rule where note_rule_id = " & pid)
            
            Case "nd_reaction"
                Call dobj.returndataset("delete from notification_reaction where reaction_id = " & pid)
                Call dobj.returndataset("delete from reaction where reaction_id = " & pid)
                
            Case "nd_comunicationserver"
                Call dobj.returndataset("delete from com_srvr_vals where com_server_id = " & pid)
                Call dobj.returndataset("delete from com_admin where com_server_id = " & pid)
                Call dobj.returndataset("delete from comunicationserver where com_server_id = " & pid)
                
            Case "nd_administrators"
                Call dobj.returndataset("delete from com_admin where administrators_id = " & pid)
                Call dobj.returndataset("delete from administrators where administrators_id = " & pid)
        
            Case "nd_schedule"
                'Verify if schedule is used for administrators
                If dobj.VerifyScheduleInAdmin(pid) Then
                    MsgBox "Cannot delete Schedule." & vbCrLf & "Schedule is referenced by Administrator(s).", vbInformation
                    IsOK = False
                Else
                    Call dobj.returndataset("delete from schedule where schedule_id = " & pid)
                End If
                
            Case "nd_analysis_rule"
                Call dobj.returndataset("delete from analysis_errdef where analysis_rule_id = " & pid)
                Call dobj.returndataset("delete from analysis_rule where analysis_rule_id = " & pid)
        
            Case "nd_collector"
                Call dobj.returndataset("delete from stat_vals where collector_id = " & pid)
                Call dobj.returndataset("delete from collector where collector_id = " & pid)
            Case "fl_collector", "fl_analysis_rule"
                Dim FolderType As enFolderType
                If "fl_collector" = Selection.ScopeItems(1).ScopeNode.NodeTypeName Then
                    FolderType = ftCollector
                Else
                    FolderType = ftAnRule
                End If
                IsOK = False
                Select Case dobj.DeleteFolder(ftCollector, CLng(pid))
                Case -1   'Ok
                    IsOK = True
                Case 1, 2
                    MsgBox "Cannot delete folder." & vbCrLf & "Please remove subfolders/subitems and try again", vbExclamation
                Case Else
                End Select
            Case "nd_data_source"
                Call dobj.returndataset("delete from data_source where data_source_id = " & pid)
        
        End Select
        
        If IsOK Then ScopeItems.Remove (Selection.ScopeItems(1).Key)

    End Select


    Set dobj = Nothing


'Select Case CurrentScopeItem.ScopeNode.NodeTypeName
'
'    Case "nd_components"
'        Set scopeitem = Selection.ScopeItems(1)
'                Debug.Print "views delete scopeitem key = " & scopeitem.key
'                'Call dobj.returndataset("delete from comp_errdef where components_id = " & pid)
'                'Call dobj.returndataset("delete from components where components_id = " & pid)
'                ScopeItems.Remove (scopeitem.key)
'
'End Select
'
'
'
'Dim scopeitem  As scopeitem
'
'    For Each scopeitem In Selection.ScopeItems
'
'        scopeitem.ScopeNode.NodeTypeName
'        ScopeItems.Remove (scopeitem.key)
'    Next scopeitem

End Sub

