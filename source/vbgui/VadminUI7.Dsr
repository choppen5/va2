    End If

Set dobj = Nothing


End Function


Sub fillsft_sub_elmnts(ByVal parentitem As ScopeItem)
    Dim returnedata As adodb.Recordset
    Dim objdaclass As VadminUI.vaDataAccess
    Dim subnode As ScopeItem
    Dim idarray As Variant
    Dim pid As Integer
    Dim etype As String
    Dim elname

    If Not parentitem.ScopeNode.ExpandedOnce Then Exit Sub

    idarray = Split(parentitem.Key, "~")
    pid = idarray(0)
    etype = idarray(1)
    Debug.Print "etype = " & etype
    etype = Trim(etype)
    elname = idarray(2)

    'If check_vlsm(pid) = False Then GoTo cleanup

    'Charles Oppenheimer - 6/12/2005 - removed function to prevent the expansion of elements if can not contact central server.
    'now just throw up warning - by calling connect_tcp - the va2 servers

    If Not (etype = "v2centralserver" Or etype = "enterprise") Then
        connect_tcp (pid)
    End If

    'determine the type of the parent_elment... if it is appserver we have several items to fill: components, tasks, etc
    Select Case etype
    Case "appserver"
        Debug.Print "expanded appserver"

'        ScopeItems.AddPreDefined "nd_server_task", pid & "~server_task~" & elname, parentitem.ScopeNode, siParent
        ScopeItems.AddPreDefined "ServerTasks", pid & "~ServerTasks~" & elname, parentitem.ScopeNode, siParent, False
        ScopeItems.AddPreDefined "Sessions", pid & "~Sessions~" & elname, parentitem.ScopeNode, siParent, False
        ScopeItems.AddPreDefined "nd_monitored_comps", pid & "~monitored_comps~" & elname, parentitem.ScopeNode, siParent
        ScopeItems.AddPreDefined "nd_processes", pid & "~processes~" & elname, parentitem.ScopeNode, siParent

    Case "vlsm"
         ScopeItems.AddPreDefined "nd_processes", pid & "~processes~" & elname, parentitem.ScopeNode, siParent

    Case "enterprise"
        Debug.Print "expanded enterprise"
'Undeclared!!!
Dim selstr
        selstr = "select * from components where sft_elmnt_id = " & pid
        'add the folder
        ScopeItems.AddPreDefined "nd_components_root", pid & "~components", parentitem.ScopeNode, siParent

        'Call addnode(parentitem, selstr, "nd_components", "components_id", "cc_alias", "description")
        'add subnode of type components - eventually, parameters?
    Case "resonate"
        Debug.Print "expanded resonate"
        ScopeItems.AddPreDefined "nd_resonate_ag", pid & "~resonate_ag~" & elname, parentitem.ScopeNode, siParent

    Case Else 'otherwise just get any sub elements related to this element
        Debug.Print "case else..."

    End Select

    'Statistics
    ScopeItems.AddPreDefined "fl_collector2", pid & "~sft~collector~" & elname, parentitem.ScopeNode, siParent, True

    Set objdaclass = New VadminUI.vaDataAccess
    Set returnedata = objdaclass.returndataset("SELECT * FROM sft_elmnt WHERE parent_elmnt_id = " & pid)
    If Not (returnedata Is Nothing) Then
        If Not (returnedata.BOF And returnedata.EOF) Then
            returnedata.MoveFirst
            Do While Not returnedata.EOF
                Set subnode = ScopeItems.AddPreDefined("nd_sft_elmnt", Trim(returnedata("sft_elmnt_id").Value) & "~" & Trim(returnedata("type")) & "~" & Trim(returnedata("name")), parentitem.ScopeNode, siParent)
                With subnode
                    .ScopeNode.displayname = Trim(returnedata("type").Value) & ":" & Trim(returnedata("name").Value)
                End With
                '.ListItems.Add allsystems("sft_mng_sys_id").Value, allsystems("name").Value, allsystems("name").Value
                returnedata.MoveNext
            Loop
        End If
    End If

cleanup:
    Set returnedata = Nothing
    Set objdaclass = Nothing
    Set subnode = Nothing
            
End Sub

Sub addnode(parentitem As ScopeItem, strsql As String, pdntype As String, idkey As String, displayname As String, ntype As String, Optional fieldlist As Variant)
            
    Dim returnedata As adodb.Recordset
    Dim objdaclass As VadminUI.vaDataAccess
    Dim subnode As ScopeItem
    Dim i As Long
    
    Set objdaclass = New VadminUI.vaDataAccess

    Set returnedata = objdaclass.returndataset(strsql)
    If Not (returnedata Is Nothing) Then
        If Not (returnedata.BOF And returnedata.EOF) Then
            returnedata.MoveFirst
            Do While Not returnedata.EOF
                Set subnode = ScopeItems.AddPreDefined(pdntype, Trim(returnedata(idkey).Value) & "~" & Trim(returnedata(ntype)), parentitem.ScopeNode, siParent)
                With subnode
                    .ScopeNode.displayname = Trim(returnedata("cc_alias").Value)
                    
                    For i = LBound(fieldlist) To UBound(fieldlist) 'add a sub item for each field in the fieldlist

                        
                         .ColumnHeaders.Add i, fieldlist(i), fieldlist(i)
                         .ListSubItems.Add i, fieldlist(i), Trim(returnedata(fieldlist(i)))
                         
                        '.ListItems(allsystems(indexfield).Value).ListSubItems.Add 2, Trim(allsystems(fieldlist(i)).Value), Trim(allsystems(fieldlist(i)).Value)

                    Next i
                            
                      
                End With
                
                returnedata.MoveNext
            Loop
        End If
    End If
                    
    Set returnedata = Nothing
    Set objdaclass = Nothing
'Undeclared! - ScopeItem
    Set subnode = Nothing 'ScopeItem

End Sub

