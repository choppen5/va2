Private Sub ResultViews_GetVirtualItemDisplayInfo(ByVal ResultView As SnapInLib.ResultView, ByVal ListItem As SnapInLib.MMCListItem)
Debug.Print "*** GetVirtualItemDisplayInfo ResultView.ScopePaneItem.Key: " & ResultView.ScopePaneItem.Key

Select Case ResultView.DisplayString
Case "Sessions"
    If Sessions.LoadValues(ListItem) Then
        ListItem.icon = 1
    End If
Case "ServerTasks"
    If ServerTasks.LoadValues(ListItem) Then
        ListItem.icon = 1
    End If
Case Else
    If ResultView.ScopePaneItem.Key = "nd_errorevent_root" Then 'Events List
        If Events.LoadValues(ListItem) Then
            ListItem.icon = 1
        End If
    End If
End Select
