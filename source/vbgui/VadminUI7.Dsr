Sub fill_collector(ByVal parentitem As ScopeItem)

    Dim allrecs As adodb.Recordset
    Dim dataobj As VadminUI.vaDataAccess
    Dim subnode As ScopeItem
    Dim ParentID As Long
    Dim pid
    Dim SftID As Long
    Dim HostID As Long
    Dim CompID As Long
    Dim FromItem As String

    If Not parentitem.ScopeNode.ExpandedOnce Then Exit Sub

    Set dataobj = New VadminUI.vaDataAccess

    ParentID = 0
    SftID = 0
    HostID = 0
    CompID = 0
    If parentitem.ScopeNode.NodeTypeName = "fl_collector2" Then
        pid = dataobj.splitval(parentitem.Key, "~", 1)
        If Not IsNumeric(pid) Then Exit Sub
        'ParentID = CLng(pid)
        FromItem = dataobj.splitval(parentitem.Key, "~", 2)
        Select Case FromItem
        Case "sft"
            SftID = CLng(pid)
        Case "host"
            HostID = CLng(pid)
        Case "comp"
            CompID = CLng(pid)
        End Select
        FromItem = FromItem & "~"
    Else
        'Fetch folders
        If parentitem.ScopeNode.NodeTypeName = "fl_collector" Then
            pid = dataobj.splitval(parentitem.Key, "~", 1)
            If IsNumeric(pid) Then
                ParentID = CLng(pid)
            End If
        End If
        Set allrecs = dataobj.GetFolders(ftCollector, ParentID)
        If Not (allrecs Is Nothing) Then
            Do Until allrecs.EOF
                Set subnode = ScopeItems.AddPreDefined("fl_collector", allrecs("folder_id").Value & "~" & "folder", parentitem.ScopeNode, siParent)
                With subnode
                    .ScopeNode.displayname = allrecs("fl_name").Value & ""
                    
                    .ColumnHeaders.Add 1, "collector_id", "Statistic Id"
                    .ColumnHeaders.Add 2, "name", "Name"
                    .ListSubItems.Add 1, Text:=allrecs("folder_id").Value & ""
                    .ListSubItems.Add 2, Text:=allrecs("fl_name").Value & ""
                End With
                allrecs.MoveNext
            Loop
            allrecs.Close
        End If
    End If

    'Fetch Nodes
    Set allrecs = dataobj.GetCollectors(ParentID, SftID, HostID, CompID)
'    Set allrecs = dataobj.returndataset("select * from collector where parent_collector_id is null")

    If Not (allrecs Is Nothing) Then
        If Not (allrecs.BOF And allrecs.EOF) Then
            'allrecs.MoveFirst
            
            Do While Not allrecs.EOF
               
                
                Set subnode = ScopeItems.AddPreDefined("nd_collector", Trim(allrecs("collector_id").Value) & "~" & FromItem & Trim(allrecs("name").Value) & "~" & "collector" & "~" & Trim(allrecs("description").Value), parentitem.ScopeNode, siParent)

                With subnode
                    .ScopeNode.displayname = Trim(allrecs("name").Value & "")
                    
                    .ColumnHeaders.Add 1, "collector_id", "Statistic Id"
                    .ColumnHeaders.Add 2, "name", "Name"
                    .ColumnHeaders.Add 3, "type", "Type"
                    .ColumnHeaders.Add 4, "active", "Active"
                    .ColumnHeaders.Add 5, "description", "Description"
                    .ColumnHeaders.Add 6, "rule_def", "Rule Deffinition"
                    .ColumnHeaders.Add 7, "error", "Error"
                    .ColumnHeaders.Add 8, "execution_interval", "Execution Interval"


                    .ListSubItems.Add 1, Text:=Trim(allrecs("collector_id").Value & "")
                    .ListSubItems.Add 2, Text:=Trim(allrecs("name").Value & "")
                    .ListSubItems.Add 3, Text:=Trim(allrecs("type").Value & "")
                    .ListSubItems.Add 4, Text:=Trim(allrecs("active").Value) & ""
                    .ListSubItems.Add 5, Text:=Trim(allrecs("description").Value & "")
                    .ListSubItems.Add 6, Text:=Trim(allrecs("rule_def").Value & "")
                    .ListSubItems.Add 7, Text:=Trim(allrecs("error").Value & "")
                    .ListSubItems.Add 8, Text:=Trim(allrecs("execution_interval").Value & "")
                    
                    
                End With
                    
                '.ListItems.Add allsystems("sft_mng_sys_id").Value, allsystems("name").Value, allsystems("name").Value
                allrecs.MoveNext
                
            Loop
        End If
            
    End If

    Set allrecs = Nothing
