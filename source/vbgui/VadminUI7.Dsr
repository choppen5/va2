Private Sub ScopeItems_PropertyChanged(ByVal ScopeItem As SnapInLib.ScopeItem, ByVal Data As Variant)
Dim vntproperties
Debug.Print "PROPERTY CHANGED!!! = " & Data & " " & ScopeItem.ScopeNode.NodeTypeName

    Select Case ScopeItem.ScopeNode.NodeTypeName

    Case "nd_sft_mng_sys_root"
        ScopeItem.RemoveChildren
        Call fillsft_mng_sys(parentitem:=ScopeItem)
    
    Case "nd_sft_mng_sys"
        If menuitem = "Edit" Then
            ScopeItem.ScopeNode.displayname = Data
        ElseIf menuitem = "Add" Then
            ScopeItem.RemoveChildren
            Call fillsft_elmnts(ScopeItem)
        End If
            
    Case "nd_sft_elmnt"
    
        If menuitem = "Add" Then
            ScopeItem.RemoveChildren
            Call create_folder(ScopeItem, "fl_sft_err_deff", "folder_sft_err_deff")
            Call fillsft_sub_elmnts(parentitem:=ScopeItem)
        ElseIf menuitem = "Edit" Then
            ScopeItem.ScopeNode.displayname = Data
        End If
            
    Case "nd_hosts"
        ScopeItem.RemoveChildren
        Call fill_hosts(parentitem:=ScopeItem)
    Case "nd_host"
        vntproperties = Split(Data, "~!~")
        ScopeItem.ScopeNode.displayname = vntproperties(0)
        
