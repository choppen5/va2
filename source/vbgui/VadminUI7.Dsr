Function connect_tcp(sft_elmnt_id As Integer) As Boolean

Dim IPAddr As Long
Dim rc As Long
Dim Host As String
Dim stat As String
Dim StartupData As WSADataType
Dim SocketNum As Long
Dim ReadSock As Long
Dim SocketBuffer As sockaddr
Dim ReadSockBuffer As sockaddr
Dim ReadBuffer As String * 2048
Dim dobj As New VadminUI.vaDataAccess
Dim program 'program will be the string we send to the ccserver
Dim sqlstr As String

Dim x As Single
Dim Y As Single
Dim bytes As Long


rc = WSACleanup()

rc = WSAStartup(&H101, StartupData)
If rc = SOCKET_ERROR Then
    Debug.Print "SOCKET_ERROR"
    ConsoleMsgBox "SOCKET_ERROR"
 Exit Function
End If
    
    
program = "CCOK" 'default
    
'Undeclared!!!
Dim rptrs
Dim ccport
Dim cchost
sqlstr = "select t1.host, t1.port from sft_elmnt t1 where t1.type = 'v2centralserver'"
Set rptrs = dobj.returndataset(sqlstr)

If Not (rptrs Is Nothing) Then
                If Not (rptrs.BOF And rptrs.EOF) Then
                    rptrs.MoveFirst
                    ccport = Trim(rptrs("port").Value)
                    cchost = Trim(rptrs("host").Value)
                    'password = Trim(rptrs("password").value)
                    
                End If
End If
    
Debug.Print "ccHost = " & cchost
Debug.Print "ccPort = " & ccport
Debug.Print "sft_elment_id checked = " & sft_elmnt_id


If sft_elmnt_id > 0 Then

    'if sft_elmnt_id is passed in we are checking on a software element
    'now we are doing a query to find the host and port of the VLSM port and host
    ' should only do this if:
    ' 1. software element has a host
    ' if it does not have a host , do not try to contact the LSM
    ' if it does have a host, try to contact the LSM
    
    
    Dim sft_element_host
    Dim sft_element_name
    
    sqlstr = "select t1.host, t1.name, t1.port from sft_elmnt t1 where t1.sft_elmnt_id = " & sft_elmnt_id
    Set rptrs = dobj.returndataset(sqlstr)
    
    If Not (rptrs Is Nothing) Then
                    If Not (rptrs.BOF And rptrs.EOF) Then
                        rptrs.MoveFirst
                        sft_element_host = Trim(rptrs("host").Value)
                        sft_element_name = Trim(rptrs("name").Value)
                    End If
    End If
        
    Debug.Print "sft element host = " & sft_element_host
    
       If (sft_element_host = "") Then
        'don't try to ping or complain about a software element with no host
        connect_tcp = False
        Exit Function
    End If
            
        
        
        
        sqlstr = "select t1.host, t1.port  from sft_elmnt t1, sft_elmnt t2 where t2.host = " _
      & "t1.host and t1.type = 'vlsm' and t2.sft_elmnt_id = " & sft_elmnt_id
    
    
    Set rptrs = dobj.returndataset(sqlstr)
    
    Debug.Print sqlstr
    
'Undeclared!!!
Dim vlsmport
Dim vlsmhost
Dim sfthost

    If Not (rptrs Is Nothing) Then
                    If Not (rptrs.BOF And rptrs.EOF) Then
                        rptrs.MoveFirst
                        vlsmport = Trim(rptrs("port").Value & "")
                        vlsmhost = Trim(rptrs("host").Value & "")
                        'password = Trim(rptrs("password").value)
                        
                    End If
    End If 'no records returned - meaning it the sql query could not find a VA2 LSM in the database for the host for this software element
    
    
    If (vlsmhost = "") Then
         ConsoleMsgBox "WARNING! The host " & sft_element_host & "" _
        & " does not appear to have a VA2 LSM installed on it.  To monitor a software element on " & sft_element_host & "" _
        & " install a VA2 LSM first.", 48
        'don't try to ping or complain about a software element with no host
        connect_tcp = False
        Exit Function
    End If
        
    program = "CCOK:" & vlsmhost & ":" & vlsmport & ":EOM"
End If ' end if sft_elment_id > 0


Debug.Print "message to CC = " & program

    IPAddr = GetHostByNameAlias(cchost)
    If IPAddr = -1 Then
        stat = "Cannot locate host " + cchost
        Debug.Print stat
        Exit Function
    End If
    SocketNum = socket(AF_INET, SOCK_STREAM, 0)
    If SocketNum = SOCKET_ERROR Then
        stat = "Cannot Create Socket."
        Debug.Print stat
        Exit Function
    End If
    SocketBuffer.sin_family = AF_INET
    SocketBuffer.sin_port = htons(ccport)
    SocketBuffer.sin_addr = IPAddr
    SocketBuffer.sin_zero = String$(8, 0)
    rc = connect(SocketNum, SocketBuffer, Len(SocketBuffer))
    If rc = SOCKET_ERROR Then
        closesocket SocketNum
        stat = "Connect rejected"
        Debug.Print stat
        ConsoleMsgBox "WARNING! THE CENTRAL SERVER CAN NOT BE CONTACTED.  MAKE SURE THE CENTRAL SERVICE IS RUNNING ON" _
        & vbCrLf & "THE HOST: " & cchost & " PORT: " & ccport, 48
        connect_tcp = False
        Exit Function
    End If
    
    rc = SendData(SocketNum, program & vbCrLf)
    
     bytes = recv(SocketNum, ReadBuffer, 2048, 0)
    If bytes > 0 Then
        program = Left$(ReadBuffer, bytes)
        stat = "Data Read"
        Debug.Print stat
        Debug.Print program
        If InStr(program, "VLSMISOK") > 0 Then
            Debug.Print "EVERY THING IS OK!!!"
            connect_tcp = True
        End If
    ElseIf WSAGetLastError() <> WSAEWOULDBLOCK Then
        x = WSAAsyncSelect(SocketNum, 0, ByVal &H202, ByVal FD_CONNECT Or FD_ACCEPT)
        stat = "Socket Closed"
        If InStr(program, Chr$(13)) <> 0 Then
            program = Left$(program, InStr(program, Chr$(13)) - 1)
        End If
        Debug.Print "would block..."
        Debug.Print stat
        
