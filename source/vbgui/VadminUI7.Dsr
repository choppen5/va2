Private Sub mn_new_folder_Click(ByVal Index As Long, ByVal Selection As SnapInLib.MMCClipboard)
Dim pid As String
Dim dobj As New vaDataAccess
Dim ScopeItem As ScopeItem
Dim ParentID As Long
Dim FolderType As enFolderType
Dim FName As String

FName = InputBox("New Folder Name", "Create Folder", "")
FName = Trim(FName)
If Len(FName) = 0 Then Exit Sub

ParentID = 0
Set ScopeItem = Selection.ScopeItems.Item(1)

Select Case ScopeItem.ScopeNode.NodeTypeName
Case "fl_collector", "fl_analysis_rule"
    pid = dobj.splitval(ScopeItem.Key, "~", 1)
    If IsNumeric(pid) Then
        ParentID = CLng(pid)
    End If
End Select

Select Case ScopeItem.ScopeNode.NodeTypeName
Case "nd_collector_root", "fl_collector"
    Call dobj.CreateFolder(ftCollector, ParentID, FName)
    ScopeItem.RemoveChildren
    Call fill_collector(ScopeItem)
Case "nd_analysis_rule_root", "fl_analysis_rule"
    Call dobj.CreateFolder(ftAnRule, ParentID, FName)
    ScopeItem.RemoveChildren
    Call fill_analysis_rule(ScopeItem)
Case Else
End Select


End Sub

