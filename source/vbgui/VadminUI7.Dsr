Function check_vlsm(element_id) As Boolean

Dim oLoc As SWbemLocator
Dim oServices As SWbemServices
Set oLoc = New SWbemLocator

Dim username
Dim Host
Dim password

Dim dobj As New VadminUI.vaDataAccess

Dim sqlstr As String
  sqlstr = "select t1.hostname, t1.username,t1.password from host t1, sft_elmnt t2, sft_elmnt t3 where t2.host = " _
  & "t1.hostname and t2.type = 'vlsm' and t2.host = t3.host And t3.sft_elmnt_id = " & element_id

'Undeclared!!!
Dim rptrs

Set rptrs = dobj.returndataset(sqlstr)


If Not (rptrs Is Nothing) Then
                If Not (rptrs.BOF And rptrs.EOF) Then
                    rptrs.MoveFirst
                    username = Trim(rptrs("username").Value)
                    Host = Trim(rptrs("hostname").Value)
                    password = Trim(rptrs("password").Value)
                    
                End If
End If

If Host <> "" Then
            
        On Error GoTo ErrorHandler
        
        Set oServices = oLoc.ConnectServer(Host, strUser:=username, _
        strPassword:=password)
        
        Dim oObjSet As SWbemObjectSet
        Set oObjSet = oServices.InstancesOf("Win32_Service")
        
        check_vlsm = False
        
'Undeclared!!!
Dim service
        For Each service In oObjSet
            If service.Name = "v2lsm" And service.State = "Running" Then
                Debug.Print "vlsm service running!"
                check_vlsm = True
            End If
            
        Next
        
        If Not check_vlsm = True Then
            ConsoleMsgBox "WARNING! Could not verify that VLSM is Running on host = " & Host
        End If

Else
    Debug.Print "this software element does not have a host defined"
    check_vlsm = True 'return true anyway... no host is uncheckable
    
End If
    


Set dobj = Nothing
Set oServices = Nothing
Set oLoc = Nothing
Set oObjSet = Nothing

Exit Function

ErrorHandler:
MsgBox "ERROR! Could not connect to host = " & Host & " with Username = " & username & "password = " & password
ccerror = 1


End Function



