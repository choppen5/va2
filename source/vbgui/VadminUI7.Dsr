Private Sub ResultViews_Activate(ByVal ResultView As SnapInLib.ResultView)

Debug.Print "*** ResultViews_Activate; ResultView.DisplayString:" & ResultView.DisplayString
Debug.Print "    ResultViews_Activate; ResultView.ScopePaneItem.Key:" & ResultView.ScopePaneItem.Key

Dim pid As String

Select Case ResultView.DisplayString
Case "Sessions"
    pid = splitval(CurrentScopeItem.Key, "~", 1)
    Set Sessions = New clsSessions
    Sessions.sft_elmnt_id = pid
    ResultView.ListView.ListItems.SetItemCount Sessions.Count
Case "ServerTasks"
    pid = splitval(CurrentScopeItem.Key, "~", 1)
    Set ServerTasks = New clsServerTasks
    ServerTasks.sft_elmnt_id = pid
    ResultView.ListView.ListItems.SetItemCount ServerTasks.Count
Case Else
    If ResultView.ScopePaneItem.Key = "nd_errorevent_root" Then
        Set Events = New clsEvents
        ResultView.ListView.ListItems.SetItemCount Events.Count
    End If
End Select
End Sub

Private Sub ResultViews_Deactivate(ByVal ResultView As SnapInLib.ResultView, Keep As Boolean)
Select Case ResultView.DisplayString
Case "ServerTasks"
    Set ServerTasks = Nothing
Case "Sessions"
    Set Sessions = Nothing
Case Else
    If ResultView.ScopePaneItem.Key = "nd_errorevent_root" Then
        Set Events = Nothing
    End If
End Select
End Sub

