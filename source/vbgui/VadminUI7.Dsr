Private Sub mn_import_collector_Click(ByVal Index As Long, ByVal Selection As SnapInLib.MMCClipboard)

Dim dobj As New vaDataAccess
Dim cmobj As New clsOpenSave
Dim filepath As String
Dim defaultpath As String
Dim ScopeItem As ScopeItem

    defaultpath = GetRegSetting(HKEY_LOCAL_MACHINE, APPKEY, "INSTALLPATH", "")
    
    With cmobj
        .DefaultExt = "xml"
        .DialogTitle = "Choose Import File"
        .Filter = "*.xml"
    '    .FilterIndex = ".xml"
        .InitDir = defaultpath
        .CancelError = True
        
        .ShowOpen
        filepath = .FileName
    End With

    Debug.Print "filepath = " & filepath

    If filepath = "" Then Exit Sub

    Dim SftID As Long
    Dim HostID As Long
    Dim CompID As Long
    Dim FolderID As Long
    Dim arr

    If Selection.SelectionType = siSingleScopeItem Then
        Set ScopeItem = Selection.ScopeItems.Item(1)
        If ScopeItem.ScopeNode.NodeTypeName = "fl_collector" Or ScopeItem.ScopeNode.NodeTypeName = "fl_collector2" Then
'            FolderID = CLng(dobj.splitval(ScopeItem.Key, "~", 1))
            arr = Split(ScopeItem.Key, "~")
            If UBound(arr) < 1 Then
                Exit Sub
            End If
        
            Select Case arr(1)
            Case "sft"
                SftID = CLng(arr(0))
            Case "host"
                HostID = CLng(arr(0))
            Case "comp"
                CompID = CLng(arr(0))
            Case "folder"
                FolderID = CLng(arr(0))
            End Select
        End If

        Call dobj.importcollector(filepath, FolderID, SftID, HostID, CompID)
    
        ScopeItem.RemoveChildren
        Call fill_analysis_rule(ScopeItem)
        
        'Undeclared!!!
        Dim Data
        CurrentScopeItem.PropertyChanged Data
    End If

End Sub

