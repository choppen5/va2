Function display_error(ScopePaneItem As SnapInLib.ScopePaneItem, errornumber As Integer) As String

    Debug.Print "running the display_error function..." & errornumber

    Select Case errornumber
        Case 1
            ScopePaneItem.DisplayMessageView "FATAL ERROR", "ERROR CONNECTING TO CENTRAL SERVER HOST. " _
            & vbNewLine & "Confirm that the Central Service software elment has the correct host listed.", siIconError
        Case 2
            ScopePaneItem.DisplayMessageView "FATAL ERROR", "THE CENTRAL SERVICE IS NOT RESPONDING. " _
            & vbNewLine & "Confirm that the Central Service is running as a service on the specified host.", siIconError
        Case 0
            ScopePaneItem.DisplayNewResultView "newview", siListView
        
    End Select

End Function

Private Sub ScopeItems_Rename(ByVal ScopeItem As SnapInLib.ScopeItem, ByVal NewName As String)
    Select Case ScopeItem.ScopeNode.NodeTypeName
    Case "fl_collector", "fl_analysis_rule"
        Dim pid
        Dim dobj As New vaDataAccess
        
        pid = dobj.splitval(ScopeItem.Key, "~", 1)
        If IsNumeric(pid) Then
            If dobj.RenameFolder(CLng(pid), NewName) Then ScopeItem.ScopeNode.displayname = NewName
        End If
    End Select
End Sub

