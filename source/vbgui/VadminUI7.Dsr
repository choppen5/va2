Private Sub mn_export_analysis_rule_Click(ByVal Index As Long, ByVal Selection As SnapInLib.MMCClipboard)
'find out if it is a multi select
'if so export each record in selected items
'export each item as adodb record set
'popup message box to ask for destination

'create array of ids, and table_name, file_name.  Pass to function create xml file based on array of ids and table name into file

Dim idarray() As Variant
Dim splitvar As Variant
Dim i As Long

Select Case Selection.SelectionType

    Case 3 'multiple items selected
    Debug.Print "MULTI SELECT!! COUNT =" & Selection.ScopeItems.Count
           
        For i = 1 To Selection.ScopeItems.Count
            ReDim Preserve idarray(1 To i)
            Debug.Print "SELECTION KEY = " & Selection.ScopeItems(i).Key
            splitvar = Split(Selection.ScopeItems(i).Key, "~")
            idarray(i) = splitvar(0)
        Next
    Case 0 'single select
        ReDim idarray(0)
        splitvar = Split(Selection.ScopeItems(1).Key, "~")
        idarray(0) = splitvar(0)
        
End Select


Dim dobj As New vaDataAccess
Dim cmobj As New clsOpenSave
Dim filepath As String
Dim defaultpath As String
defaultpath = GetRegSetting(HKEY_LOCAL_MACHINE, APPKEY, "INSTALLPATH", "")

With cmobj
    .DefaultExt = "xml"
    .DialogTitle = "Choose Export File Name"
    .Filter = "xml"
    .InitDir = defaultpath
    .ShowSave
    filepath = .FileName
End With

Debug.Print "filepath = " & filepath
Debug.Print "cancle error = " & cmobj.CancelError

If cmobj.CancelError = False Then
    Call dobj.makexml(idarray, splitvar(2), splitvar(2) & "_id", filepath)
End If
End Sub

Private Sub mn_import_analysis_rule_Click(ByVal Index As Long, ByVal Selection As SnapInLib.MMCClipboard)
