Private Sub Views_Paste(ByVal DestView As SnapInLib.View, ByVal SourceItems As SnapInLib.MMCClipboard, ByVal DestScopeItem As SnapInLib.ScopeItem, ByVal RetToSource As SnapInLib.MMCDataObject, ByVal Move As Boolean)
Debug.Print "Views_Paste"
    If Not SourceItems.SelectionType = siSingleScopeItem Then Exit Sub
    'If SourceItems.ScopeItems.Count = 0 Then Exit Sub
'VB.Clipboard.SetText "Views_Paste"
    Dim ScopeItem As ScopeItem
    Dim dobj As New vaDataAccess
    Dim NewParentID As Long
    Dim SourceID As Long
    Dim pid
    
    Set ScopeItem = SourceItems.ScopeItems.Item(1)
    Select Case ScopeItem.ScopeNode.NodeTypeName
    Case "nd_collector", "nd_analysis_rule"
        If Not Move Then Exit Sub
        Select Case DestScopeItem.ScopeNode.NodeTypeName
        Case "nd_collector_root", "nd_analysis_rule_root"
            NewParentID = 0
        Case "fl_collector", "fl_analysis_rule"
            pid = dobj.splitval(DestScopeItem.Key, "~", 1)
            If Not IsNumeric(pid) Then Exit Sub
            NewParentID = CLng(pid)
        Case Else
            Exit Sub
        End Select
        
        pid = dobj.splitval(ScopeItem.Key, "~", 1)
        If Not IsNumeric(pid) Then Exit Sub
        SourceID = CLng(pid)
    End Select
    
    Select Case ScopeItem.ScopeNode.NodeTypeName
    Case "nd_collector"
        If dobj.ChangeCollectorFolder(SourceID, NewParentID) Then
            ScopeItem.Pasted = True
            ScopeItems.Remove ScopeItem.Key
            DestScopeItem.RemoveChildren
            fill_collector DestScopeItem
        End If
    Case "nd_analysis_rule"
        If dobj.ChangeAnRuleFolder(SourceID, NewParentID) Then
            ScopeItem.Pasted = True
'            Dim subnode As ScopeItem
'            Set subnode = ScopeItems.AddPreDefined("nd_analysis_rule", dobj.splitval(ScopeItem.Key, "~", 1) & "~" & dobj.splitval(ScopeItem.Key, "~", 2) & "~" & "analysis_rule" & IIf(NewParentID > 0, "~" & NewParentID, ""), DestScopeItem.ScopeNode, siParent)
            
            ScopeItems.Remove ScopeItem.Key
            DestScopeItem.RemoveChildren
            fill_analysis_rule DestScopeItem
        End If
    End Select
End Sub

