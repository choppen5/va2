Sub fill_graph(ByVal ResultView As SnapInLib.ResultView, pid, title As String, description)

 
Dim rptrs As New adodb.Recordset
Dim dobj As New VadminUI.vaDataAccess
Dim i As Long

Set rptrs = dobj.returndataset("select * from stat_vals where collector_id = " & pid & " order by stat_vals_id DESC")
'"select * from stat_vals where collector_id = " & pid)


With ResultView.Control
 
Dim d As Double
Dim dt As Double
    dt = Now
    
    
    .Subsets = 1
    .Points = rptrs.RecordCount
    
    '** Enable double precision which is usually **'
    '** required for date time handling **'
    '** This means we pass x data to XDataII **'
    .UsingXDataII = True


    If Not (rptrs Is Nothing) Then
                If Not (rptrs.BOF And rptrs.EOF) Then
                    rptrs.MoveFirst
                    
                    i = 0
            
                    Do While Not rptrs.EOF
                            '** Pass XDataII and YData **'
                            
                                d = rptrs("time_stmp").Value
                                'Debug.Print "d = " & d
                                '(dt + (p * 0.25))
                                .XDataII(0, i) = d
                                .YData(0, i) = rptrs("val").Value
                                '(150 + (Sin(p * 0.034) * 60) + (Rnd * 30) + (p / 2)) - (35 + (Rnd * 20))
                            'Next p
                    i = i + 1
                            
                        rptrs.MoveNext
                    
                    
                    Loop
                 End If
    End If

    '** Enable DateTimeMode **'
   .DateTimeMode = PEDTM_VB
    
    '** Allow zooming and panning **'
    .AllowZooming = PEAZ_HORIZONTAL
    .ScrollingHorzZoom = True
    .AllowDataHotSpots = True
    
    .MainTitle = title
    .SubTitle = description
    
    
    '** Cursor prompting in top left corner **'
    .CursorMode = PECM_DATASQUARE
    .CursorPromptTracking = True
    .CursorPromptStyle = PECPS_XYVALUES
       
    '** Set some various properties **'
    .DeskColor = RGB(192, 192, 192)
    .PrepareImages = True
    .MarkDataPoints = True
   
End With

Set rptrs = Nothing
Set dobj = Nothing

End Sub

